{
    "name": "api",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "deploy": "wrangler publish",
        "start": "wrangler dev",
        "format": "npx prettier --write ."
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20230419.0",
        "typescript": "^5.0.4",
        "wrangler": "^3.0.0"
    },
    "prettier": {
        "arrowParens": "always",
        "bracketSameLine": false,
        "bracketSpacing": true,
        "insertPragma": false,
        "jsxSingleQuote": false,
        "printWidth": 500,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "semi": true,
        "singleQuote": false,
        "tabWidth": 4,
        "trailingComma": "es5",
        "useTabs": false,
        "vueIndentScriptAndStyle": false
    }
